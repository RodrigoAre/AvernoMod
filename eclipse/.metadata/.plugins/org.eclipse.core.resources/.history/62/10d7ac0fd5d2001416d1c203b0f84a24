package com.averno.item;

import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;

/**
 * 
 * @author darkraksis	
 * Esta es una clase de objeto genérico de ejemplo, un objeto debe heredar de la clase Item.
 */
public class GenericItem extends Item {

	/**
	 * Constructor vacío, en él se inician los datos necesarios para el objeto.</br>
	 */
	public GenericItem() {
		setMaxStackSize(64);
		setCreativeTab(CreativeTabs.tabMisc);
		setUnlocalizedName("genericItem");
	}
	
}
